
<% @posts.each do |post| %>
<div class="post">
  <h2 class="title"><%= link_to post.title,post %></h2>
  <p class="entry"><%= post.body %></p>
  <% if post.admin_user %>   
    <p> Posted by <%= post.admin_user.name%></p>   
  <% end %>  
  <div class="byline">
    <p class="meta"><%= post.created_at.strftime("%d %b, %Y")%> </p>     
    <p class="links"><%= link_to "Read More",post %></p>
  </div>
</div>

<% end %>
